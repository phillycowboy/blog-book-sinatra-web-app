<div class="genre-container">
    <h1>Genres</h1>
    <div class="genre-cards">
        <% @genres.each do |genre|%>
            <% if genre.name != "" || nil %>
                <div class="genre-card">
                <h2><%= genre.name %></h2>
                </div>
            <%end%>
        <%end%>
    </div>
</div>